<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.makeanandroidapplikealiexpressamazon_.ui.home.HomeFragment"
         tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.makeanandroidapplikealiexpressamazon_.ui.gallery.GalleryFragment"
         tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.makeanandroidapplikealiexpressamazon_.ui.slideshow.SlideshowFragment"
         tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_tools"
        android:name="com.example.makeanandroidapplikealiexpressamazon_.ui.tools.ToolsFragment"

        tools:layout="@layout/fragment_tools" />

    <fragment
        android:id="@+id/nav_share"
        android:name="com.example.makeanandroidapplikealiexpressamazon_.ui.share.ShareFragment"
         tools:layout="@layout/fragment_share" />

    <fragment
        android:id="@+id/nav_send"
        android:name="com.example.makeanandroidapplikealiexpressamazon_.ui.send.SendFragment"
         tools:layout="@layout/fragment_send" />
</navigation>